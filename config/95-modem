uci set network.Modem=interface
uci set network.Modem.proto=modemmanager
uci set network.Modem.device=/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.1
uci set network.Modem.apn=${MODEM_APN}
uci add_list network.Modem.allowedauth=none
uci set network.Modem.iptype=ipv4
uci set network.Modem.loglevel=ERR
uci set network.wan.peerdns='0'
uci set network.wan6.peerdns='0'
uci commit network
service network restart
